{
	"name": "firefish-js",
	"version": "0.0.27",
	"description": "Firefish SDK for JavaScript",
	"homepage": "https://git.joinfirefish.org/firefish/firefish/-/tree/develop/packages/firefish-js",
	"main": "./built/index.js",
	"types": "./built/index.d.ts",
	"license": "MIT",
	"scripts": {
		"build": "pnpm swc src -d built -D",
		"build:types": "pnpm tsc --emitDeclarationOnly",
		"build:debug": "pnpm swc src -d built -s -D",
		"lint": "pnpm biome check --apply src",
		"format": "pnpm biome format --write src",
		"jest": "jest --coverage --detectOpenHandles",
		"test": "pnpm jest && pnpm tsd"
	},
	"repository": {
		"type": "git",
		"url": "https://git.joinfirefish.org/firefish/firefish.git"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.63",
		"@swc/core": "1.3.78",
		"@swc/types": "^0.1.5",
		"@types/jest": "^29.5.8",
		"@types/node": "20.9.0",
		"jest": "^29.7.0",
		"jest-fetch-mock": "^3.0.3",
		"jest-websocket-mock": "^2.5.0",
		"mock-socket": "^9.3.1",
		"ts-jest": "^29.1.1",
		"ts-node": "10.9.1",
		"tsd": "^0.29.0",
		"typescript": "5.2.2"
	},
	"files": [
		"built"
	],
	"dependencies": {
		"eventemitter3": "^5.0.1",
		"reconnecting": "^4.4.1"
	},
	"optionalDependencies": {
		"@swc/core-android-arm64": "1.3.11"
	}
}
